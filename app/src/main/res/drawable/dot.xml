<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:left="1dp"
        android:right="1dp"
        android:bottom="1dp"
        android:top="1dp">

        <shape>
            <stroke
                android:width="16dp"
                android:color="#FF0101"
                android:dashGap="6dp"
                android:dashWidth="5dp"/>
            <corners android:radius="100dp"/>
        </shape>
    </item>
</layer-list>